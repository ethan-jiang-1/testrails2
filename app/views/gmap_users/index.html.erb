<h2>Listing gmap_users</h2>

<table class="table">
  <tr>
    <th>name</th>
    <th>address</th>
    <th>latitude</th>
    <th>longitude</th>
  </tr>

<% @gmap_users.each do |gmap_user| %>
  <tr>
    <td><%= gmap_user.name %></td>
    <td><%= gmap_user.address %></td>
    <td><%= gmap_user.latitude %></td>
    <td><%= gmap_user.longitude %></td>
    <td><%= link_to 'Show', gmap_user %></td>
    <td><%= link_to 'Edit', edit_gmap_user_path(gmap_user) %></td>
    <td><%= link_to 'Destroy', gmap_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Gmap user', new_gmap_user_path, :class=>"btn btn-primary" %>
